<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net40;net45;netstandard1.3;netstandard2.0</TargetFrameworks>
    <AssemblyName>Mondop.Core</AssemblyName>
    <RootNamespace>Mondop.Core</RootNamespace>
    <Title>Mondop.Core</Title>
    <Version>1.2.2</Version>
    <Authors>Marcel Vroone</Authors>
    <Company>It-actief</Company>
    <Product>Mondop</Product>
    <Copyright>Copyright 2020 by It-actief</Copyright>
    <PackageProjectUrl>http://www.mondop.com</PackageProjectUrl>
    <RepositoryUrl>https://github.com/MondopDotCom/mondop.core</RepositoryUrl>
    <PackageLicenseUrl></PackageLicenseUrl>
    <Description>A package containing some extension classes</Description>
    <PackageReleaseNotes>1.0.0 - Added Ensure.IsNotNull
1.0.1 - Corrected casing to Camel
1.1.0 - Moved to .net standard 1.3, added IFile and IDirectory
1.1.1 - Updated Mondop.Abstraction package to 1.0.3
1.1.2 - Added IEnumerable&lt;string&gt;.Join extension
1.2.0 - IFile, IDirectory moved to Mondop.Mockables
1.2.1 - Added Multiple Targets
1.2.2 - Added StringBuilderExtensions</PackageReleaseNotes>
    <AssemblyVersion>1.2.1.0</AssemblyVersion>
    <FileVersion>1.2.1.0</FileVersion>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Mondop.Guard" Version="1.0.1" />
  </ItemGroup>

</Project>
